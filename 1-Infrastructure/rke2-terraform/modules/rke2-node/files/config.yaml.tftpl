%{ if primary == true }
bind-address: ${ip_addr}
%{ else }
server: https://${server_addr}:9345
%{ endif }
%{ if role == "server" }
write-kubeconfig-mode: "0644"
tls-san:
  - k8s.kellerhome.us
%{ endif }
token: my-shared-secret
disable:
  - rke2-metrics-server
  - rke2-ingress-nginx
disable-cloud-controller: true